type anx_check, domain, coredomain;
type anx_check_exec, system_file_type, exec_type, file_type;

allow anx_check media_rw_data_file:dir { getattr open read };
allow anx_check media_rw_data_file:file { getattr open read };
allow anx_check mnt_user_file:dir search;
allow anx_check mnt_user_file:lnk_file read;
